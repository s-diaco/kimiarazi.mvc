@section head{
    <style>
        header {
            background-image: url('../../Images/Shared/header-bg5.jpg');
        }
    </style>

    <title>@Resources.DigitalInk.PageTitle</title>
}

@section Scripts{
    <!-- animated scroll loading effect -->
    <script type="text/javascript">
        jQuery(document).ready(function () {
            jQuery('.post').addClass("not-visible").viewportChecker({
                classToAdd: 'visible animated bounceInUp',
                offset: 100
            });
        });
    </script>
}

<!-- Colors Grid Section -->

<section id="colors" class="bg-light-gray">
    <div class="container">
        <div class="row post">
            <div class="col-xs-12">
                <img class="article-img-1 pull-right" src="~/Images/article/ink.jpg" />
                <h2 class="section-heading"><i class="fa fa-google-wallet"></i> @Resources.DigitalInk.Head</h2>
                @Html.Raw(Resources.DigitalInk.Desc)
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <h2 class="section-heading post">
                    <i class="fa fa-quote-right"></i>&nbsp;@Resources.DigitalInk.WhyHead&nbsp;<i class="fa fa-quote-left"></i>
                </h2>
                <ul class="tick-list">
                    <li>
                        <i class="fa fa-check-circle"></i>
                        @Resources.DigitalInk.WhyList1
                    </li>
                    <li>
                        <i class="fa fa-check-circle"></i>
                        @Resources.DigitalInk.WhyList2
                    </li>
                    <li>
                        <i class="fa fa-check-circle"></i>
                        @Resources.DigitalInk.WhyList3
                    </li>
                    <li>
                        <i class="fa fa-check-circle"></i>
                        @Resources.DigitalInk.WhyList4
                    </li>
                    <li>
                        <i class="fa fa-check-circle"></i>
                        @Resources.DigitalInk.WhyList5
                    </li>
                    <li>
                        <i class="fa fa-check-circle"></i>
                        @Resources.DigitalInk.WhyList6.
                    </li>
                    <li>
                        <i class="fa fa-check-circle"></i>
                        @Resources.DigitalInk.WhyList7
                    </li>
                    <li>
                        <i class="fa fa-check-circle"></i>
                        @Resources.DigitalInk.WhyList8
                    </li>
                    <li>
                        <i class="fa fa-check-circle"></i>
                        @Resources.DigitalInk.WhyList9
                    </li>
                </ul>
            </div>
        </div>
    </div>
</section>
